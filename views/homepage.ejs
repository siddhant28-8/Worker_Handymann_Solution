    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@200;300;400;500;700&family=Poppins:wght@400;500;600;700&display=swap" rel="stylesheet">
    
    <style>
        :root {
        --primary-purple: #9100FF;
        --dark-background: #1c1c1c;
        --light-text: #f8f9fa;
        }
     
        body{
            background-color: #E7E7E7;
        }
	 /* Top navbar */
	 /* Styling for the Post-Login Dropdown Menu */
   /* Top navbar */
    .navbar {
      background-color: #000 !important;
    }
    .navbar .nav-link{
     color: #9100FF !important;
    }
    .navbar .nav-link:hover, .navbar .nav-item.active .nav-link{
      color: #9100FF !important;
    }
    .navbar-toggler-icon {
      filter: invert(1);
      color: #9100FF !important;
    }

    .navbar-brand img {
       height: 40px; /* Adjust logo size */
       margin-right: 5px;
    }
    
    /* Styling for the Post-Login Dropdown Menu Container */
    .dropdown-menu {
        border-radius: 8px; /* Rounded corners for modern look */
        box-shadow: 0 4px 10px rgba(0, 0, 0, 0.5); /* Soft shadow */
        padding: 0; /* Remove default padding for full-height items */
        background-color:var(--primary-purple) ;
    }
    
    /* Style for the Header (User Role and Name) */
    .dropdown-header {
        color: #333;
        font-weight: 600;
        padding: 0.75rem 1rem 0.5rem;
        white-space: nowrap; /* Prevents long names from wrapping */
    }
    
    /* Style for individual Dropdown Items (links) */
    .dropdown-item {
        padding: 0.75rem 1rem;
        transition: background-color 0.3s;
    }
    
    /* Hover effect for Dropdown Items */
    .dropdown-item:hover {
        background-color: var(--primary-purple); /* Background hover matching the theme */
        color: white !important; /* Text color change on hover to white */
    }
    
    /* Style for the Logout Button icon spacing */
    .dropdown-logout-btn i {
        margin-right: 8px;
    }
    
    /* Ensure the profile icon itself maintains the purple color in the navbar */
    /* This is important to ensure the icon is always purple even if other nav links are white */
    .nav-item.dropdown .nav-link i {
        color: var(--primary-purple) !important; 
    }

    .navbar-profile-img {
      width: 36px;
      height: 36px;
      border-radius: 50%;
      object-fit: cover;
      border: 2px solid var(--primary-purple);
      background-color: #fff;
      box-shadow: 0 0 6px rgba(145, 0, 255, 0.3);
      transition: transform 0.2s ease;
    }
    
    .navbar-profile-img:hover {
      transform: scale(1.05);
    }

    .btn-login {
            background-color: var(--primary-purple);
            color:white;
            padding: 0.375rem 1rem;
            border-radius: 0.25rem;
            text-decoration: none !important;
            font-weight: 500;
            margin-left: 15px;
        }

        /* --- Main Carousel Section Styling --- */
        #carouselExampleIndicators .carousel-item img {
            width: 100%;
            height: 550px; 
            object-fit: cover; 
            filter: brightness(70%); 
        }
        .carousel-caption {
            top: 50%; 
            transform: translateY(-50%);
            left: 10%; 
            right: auto;
            text-align: left;
            padding-bottom: 0;
            color: white; 
        }
        .carousel-caption b {
            font-size: 2.8rem;
            font-weight: 700;
            line-height: 1.2;
            margin-bottom: 1rem;
            display: block;
        }
        .carousel-caption p {
            font-size: 1rem;
            max-width: 500px;
        }
        .carousel-indicators li {
            background-color: white;
            opacity: 0.5;
        }
        .carousel-indicators .active {
            opacity: 1;
            background-color: var(--primary-purple);
        }

        /* --- User/Worker Cards Section Styling --- */
        #mid {
            padding: 4rem 0;
            background-color:#E7E7E7;
        }
        .card-register {
            text-align: center;
            padding: 2rem;
            border: none;
            background-color:#E7E7E7;
            margin: 1rem;
            border-radius: 15px;
            min-height: 350px; 
            box-shadow: 0 4px 10px rgba(8, 8, 8, 0.05);
        }
        

        .img-mid {
            border-radius: 50%;
            height: 150px;
            width: 150px;
            object-fit: cover;
            margin-bottom: 1rem;
        }
        .card-register .btn {
            background-color: var(--dark-background);
            color: white;
            border-radius: 5px;
            padding: 0.5rem 2rem;
            margin-top: 1rem;
        }

        /* --- Services Section Styling (Carousel) --- */
        #services {
            padding: 4rem 0;
            text-align: center;
        }
        #services h2 {
            color: var(--primary-purple);
            font-weight: 700;
            margin-bottom: 3rem;
        }
        /* Group of cards in one carousel item */
        .service-card-row {
            display: flex; /* Use flex to align cards within the row */
            justify-content: center; /* Center the group of cards */
            align-items: flex-start;
            padding: 0 50px; /* Add padding to prevent cards from touching the controls */
        }
        .service-card-container {
            /* This is the column that holds each card. We set its width here. */
            padding: 0 15px; /* Spacing between cards */
        }
        .service-card {
            text-align: center;
            margin: 0; /* Margin handled by col padding */
            background:#E7E7E7; /* Ensure card background is clear */
            padding: 20px 0; /* Padding inside the card area */
        }
        .service-card .image img {
            height: 140px;
            width: 140px;
            border-radius: 50%;
            object-fit: cover;
            margin-bottom: 1rem;
            border: 5px solid #eee; 
        }
        .service-card .btn {
            background-color: var(--dark-background);
            color: white;
            border-radius: 5px;
            padding: 0.5rem 1.5rem;
            margin-top: 0.5rem;
        }
        
     /* ðŸŽ¨ UPDATED: Style for the service carousel controls - Clean White Circle with Purple Arrow ðŸŽ¨ */
     /* Service Carousel Controls - Clean default arrows */
       #servicesCarousel .carousel-control-prev,
       #servicesCarousel .carousel-control-next {
           width: 5%;
           background: none;
           border: none;
           opacity: 0.8;
           transition: opacity 0.3s;
       }
       #servicesCarousel .carousel-control-prev:hover,
       #servicesCarousel .carousel-control-next:hover {
           opacity: 1;
       }
       
       /* Bigger black arrows, properly centered */
       #servicesCarousel .carousel-control-prev-icon,
       #servicesCarousel .carousel-control-next-icon {
           background-image: none; /* remove Bootstrap default */
           width: 50px;
           height: 50px;
       }
       
       /* Left arrow */
       #servicesCarousel .carousel-control-prev-icon::after {
           content: 'â€¹';
           font-size: 50px;
           color: #000;
           font-weight: bold;
           line-height: 50px;
       }
       
       /* Right arrow */
       #servicesCarousel .carousel-control-next-icon::after {
           content: 'â€º';
           font-size:50px;
           color: #000;
           font-weight: bold;
           line-height: 50px;
       }


        /* End of control styling update */


        /* --- Why Choose Us Section Styling --- */
        #level3 {
            padding: 4rem 0;
        }
        #level3 h2 {
            font-size: 2.5rem;
            font-weight: 700;
            margin-bottom: 2rem;
        }
        #level3 .col-md-5 {
            padding-right: 4rem;
        }
        #level3 .col-md-7 img {
            border-radius: 10px;
            object-fit: cover;
        }
        .feature-item {
            display: flex;
            align-items: center;
            margin-bottom: 1.5rem;
        }
        .feature-item img {
            height: 50px;
            width: 50px;
            border-radius: 50%;
            object-fit: cover;
            margin-right: 15px;
        }
        .feature-item span {
            font-size: 1.1rem;
            font-weight: 500;
        }

        /* --- How It Works Section Styling --- */
        #last {
            padding: 4rem 0;
            text-align: center;
            background-color:#E7E7E7;
        }
        #last h2 {
            color: var(--primary-purple);
            font-weight: 700;
            margin-bottom: 0.5rem;
        }
        #last p:nth-of-type(1) {
            margin-bottom: 3rem;
        }
        #last .col-md-4 {
            padding: 0 2rem;
        }

        /* --- Footer Styling --- */
        .footer {
            background-color: var(--dark-background);
            color: white;
            padding: 40px 10px;
        }
        .footer-container {
            display: flex;
            justify-content: space-between;
            flex-wrap: wrap;
            max-width: 1200px;
            margin: auto;
        }
        .footer-left {
            flex: 1;
            min-width: 200px;
            margin: 10px;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            border-right: 1px solid #706f6f;
        }
        .footer-left img {
            height: auto;
            width: auto;
            margin-bottom: 5px;
        }

        .footer-center{ 
            flex: 1;
            min-width: 200px;
            margin: 10px;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            border-right: 1px solid #706f6f;
        }
        .footer-center ul { 
            list-style: none; 
            padding: 0; 
            line-height: 2.2; 
        }
        .footer-center ul li a { 
            text-decoration: none; 
            color: white; 
            font-size: 14px; 
            transition: color 0.3s; 
        }
        .footer-center ul li a:hover { 
            color: var(--primary-purple); 
        }

        .footer-right { 
            flex: 1;
            min-width: 200px;
            margin: 10px;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            gap: 30px; 
        }
        .footer-right .social { 
            display: flex; 
            align-items: center; 
            font-size: 14px; 
        }
        .footer-right .social span { 
            margin-right: 10px; 
            color: #ccc; 
            min-width: 70px; 
            text-align: right;
        }
        .footer-right .social i { 
            font-size: 16px; 
            color: var(--primary-purple); 
            background: white; 
            border-radius: 4px; 
            padding: 5px; 
            transition: transform 0.3s; 
            margin-left: 10px; 
        }
        .footer-right .social i:hover { 
            transform: scale(1.1); 
        }
        .tagline { 
            font-size: 13px; 
            color: #ccc; 
            margin-top: 8px; 
        }

        @media (max-width: 992px) {
            /* On smaller screens, allow one card per row in service carousel */
            .service-card-row {
                flex-direction: column;
                align-items: center;
                padding: 0;
            }
            .service-card-container {
                max-width: 300px;
                margin-bottom: 20px;
            }
            .footer-left, .footer-center, .footer-right { border-right: none; margin-bottom: 20px; }
            .carousel-caption { left: 5%; right: 5%; text-align: center; }
            .carousel-caption b { font-size: 2rem; }
        }
        @media (min-width: 992px) {
            /* On large screens, enforce 3 cards per row */
            .service-card-container {
                flex: 0 0 33.333333%;
                max-width: 33.333333%;
            }
        }
    </style>
</head>
